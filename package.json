{
  "name": "rmagent",
  "version": "0.0.1",
  "description": "Rack Manage CLI agent for monitoring and interacting with on-premises equipment",
  "homepage": "https://github.com/cdgco/RackManage-Agent",
  "bugs": "https://github.com/cdgco/RackManage-Agent/issues",
  "repository": "cdgco/RackManage-Agent",
  "license": "MIT",
  "author": "Rack Manage",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "rmagent": "./bin/run.js"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json",
    "/theme.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "prepack": "yarn run build && oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepare": "yarn run build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "yarn run lint",
    "version": "shx rm -rf docs && oclif readme --multi && git add README.md"
  },
  "oclif": {
    "bin": "rmagent",
    "commands": "./dist/commands",
    "dirname": "rmagent",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-update",
      "@oclif/plugin-version",
      "@oclif/plugin-not-found"
    ],
    "topicSeparator": " ",
    "topics": {
      "server": {
        "description": "Manage servers monitored by the agent"
      },
      "service": {
        "description": "Manage the agent background service"
      },
      "ipmi": {
        "description": "Manage IPMI settings for servers"
      },
      "plugins": {
        "description": "Manage plugins for the agent"
      }
    },
    "macos": {
      "identifier": "io.rackmanage.rmagent",
      "sign": "\"Developer ID Installer: Carter Roeser (87D84YS9QF)\""
    },
    "windows": {
      "name": "Rack Manage Agent"
    },
    "update": {
      "s3": {
        "host": "https://agent.rackmanage.io",
        "bucket": "rmagent",
        "xz": true
      }
    },
    "additionalVersionFlags": [
      "-v"
    ],
    "additionalHelpFlags": [
      "-h"
    ],
    "theme": "theme.json"
  },
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-not-found": "^3.1.1",
    "@oclif/plugin-update": "^4.2.3",
    "@oclif/plugin-version": "^2.0.16",
    "@oclif/plugin-warn-if-update-available": "^3.0.15",
    "cli-table": "^0.3.11",
    "cli-table3": "^0.6.4",
    "firebase": "^10.10.0",
    "jsonwebtoken": "^9.0.2",
    "keytar": "^7.9.0",
    "needle": "^3.3.1",
    "ping-monitor": "^0.8.2",
    "promptly": "^3.2.0",
    "sqlite3": "^5.1.7",
    "winreg": "^1.2.5"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^10.0.6",
    "@types/node": "^18",
    "@types/promptly": "^3.0.5",
    "chai": "^5.1.0",
    "eslint": "^8.57.0",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
